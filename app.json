{
  "expo": {
    "name": "crypto-tracker-app",
    "slug": "crypto-tracker-app",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Learning to use react-native (and a bit of redux) by making a cryptocurrency price checker (api returns snapshot of old data as didnt want to pay for requests etc)"
  },
  "name": "crypto-tracker-v1"
}
